{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "import/prefer-default-export": "off",
        "no-await-in-loop": "off"
    },
    "plugins": ["jasmine", "codeceptjs"]
}
